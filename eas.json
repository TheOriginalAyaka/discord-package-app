{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6751523499"
      },
      "android": {
        "track": "production"
      }
    },
    "beta": {
      "ios": {
        "ascAppId": "6751523499"
      },
      "android": {
        "track": "beta",
        "releaseStatus": "completed"
      }
    },
    "internal": {
      "ios": {
        "ascAppId": "6751523499"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
