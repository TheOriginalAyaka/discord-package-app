{
  "name": "discord-package-app",
  "version": "0.8.1",
  "main": "index.ts",
  "author": {
    "name": "TheOriginalAyaka"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "format": "npm run format:js && npm run format:rust",
    "format:js": "npx biome format --write .",
    "format:rust": "cargo fmt --manifest-path dpkg-rs/Cargo.toml --all",
    "lint": "npm run lint:js && npm run lint:rust",
    "lint:js": "npx biome lint --write .",
    "lint:rust": "cargo clippy --manifest-path dpkg-rs/Cargo.toml --all-targets -- -D warnings"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.2",
    "@react-native/assets-registry": "^0.81.1",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "expo": "~54.0.10",
    "expo-application": "~7.0.7",
    "expo-asset": "~12.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-device": "~8.0.8",
    "expo-document-picker": "~14.0.7",
    "expo-font": "~14.0.8",
    "expo-image": "~3.0.8",
    "expo-native-storage": "^0.1.0",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "2.2.2",
    "@types/react": "~19.1.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "typescript": "~5.9.2"
  }
}
