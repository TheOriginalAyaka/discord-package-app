{
  "expo": {
    "name": "Dispackage",
    "slug": "discord-package-app",
    "version": "0.5.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B1D22"
    },
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theoriginalayaka.dpkg",
      "appleTeamId": "LX2U7VU2K4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": {
        "light": "./assets/ios-light.png",
        "dark": "./assets/ios-dark.png",
        "tinted": "./assets/ios-tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5865F2",
        "monochromeImage": "./assets/adaptive-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.theoriginalayaka.dpkg"
    },
    "extra": {
      "eas": {
        "projectId": "71f1320f-8412-4f74-a631-3106bcdd1ba5"
      }
    },
    "owner": "ayaka-one",
    "plugins": [
      [
        "expo-font",
        {
          "android": {
            "fonts": [
              {
                "fontFamily": "ABCGintoNordUnlicensedTrial-Black",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/abc_ginto.otf",
                    "weight": 800
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": ["./assets/fonts/abc_ginto.otf"]
          }
        }
      ]
    ]
  }
}
