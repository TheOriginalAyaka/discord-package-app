{
  "expo": {
    "name": "Dispackage",
    "slug": "discord-package-app",
    "version": "0.6.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theoriginalayaka.dpkg",
      "appleTeamId": "LX2U7VU2K4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": "./assets/dispackage.icon"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5865F2",
        "monochromeImage": "./assets/adaptive-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.theoriginalayaka.dpkg"
    },
    "extra": {
      "eas": {
        "projectId": "71f1320f-8412-4f74-a631-3106bcdd1ba5"
      }
    },
    "owner": "ayaka-one",
    "plugins": [
      [
        "expo-font",
        {
          "android": {
            "fonts": [
              {
                "fontFamily": "ABCGintoNordUnlicensedTrial-Black",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/abc_ginto.otf",
                    "weight": 800
                  }
                ]
              },
              {
                "fontFamily": "ggsans-Regular",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/ggsans_regular.ttf",
                    "weight": 400
                  }
                ]
              },
              {
                "fontFamily": "ggsans-Medium",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/ggsans_medium.ttf",
                    "weight": 500
                  }
                ]
              },
              {
                "fontFamily": "ggsans-Semibold",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/ggsans_semibold.ttf",
                    "weight": 600
                  }
                ]
              },
              {
                "fontFamily": "ggsans-Bold",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/ggsans_bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/abc_ginto.otf",
              "./assets/fonts/ggsans_regular.ttf",
              "./assets/fonts/ggsans_medium.ttf",
              "./assets/fonts/ggsans_semibold.ttf",
              "./assets/fonts/ggsans_bold.ttf"
            ]
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "ios": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#1B1D22"
          },
          "android": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#1B1D22",
            "imageWidth": 150
          }
        }
      ]
    ]
  }
}
